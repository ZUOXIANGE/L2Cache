{
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "L2Cache": {
    "DefaultExpirationMinutes": 30,
    "EnableLocalCache": true,
    "EnableRedisCache": true,
    "KeyPrefix": "Advanced:L2Cache:"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "L2Cache": "Information"
    }
  },
  "AllowedHosts": "*"
}
